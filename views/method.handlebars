<main class="center">
      <div class="row">
        <div id="main-header" class="center">        
          
          <div class="col s12 m6 offset-m3">
              <div class="card card-content transparent">

                <span><button type="button" class="blue-grey btn" id="likes">
                  <i class="far fa-thumbs-up"></i>
                </button>#{{likes}}</span>

                <h1 class="method-title">{{name}}(&nbsp;&nbsp;)</h1>
                
                <span><button type="button" class="blue-grey btn" id="dislikes">
                  <i class="far fa-thumbs-down"></i>
                </button>#{{dislikes}}</span>

                <div>
                <span><button type="button" class="blue-grey btn" id="newMethod">
                  Generate a New Method
                </button></span>
                </div>

              </div>
          </div>         
        </div>
      </div>
<!--Test change for Git hub, ignore this comment-->
<div class="row">
          <div class="col s12 m6 offset-m3">
            <div class="card blue-grey darken-1">
              <div class="card-content white-text">
                <span class="card-title">General</span>
                {{!-- current object.category --}}
                <p>Category: {{category}}</p>
                
              </div>
            </div>
          </div>
</div>

<div class="row">
  <div class="col s12 m6 offset-m3">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Syntax</span>
          <p>{{syntax}}</p>
            <hr>
              {{!-- if the .parameters inside the object were passing hbs exists
                    handle bars willl only execute this block if it is not empty, null, or ''.

                 --}}
                    {{#if parameters}}
                    <p>Arguments/Params:</p>

                    <ul>
                      {{!-- for each element in the parameters array: --}}
                      {{#each parameters}}
                      {{!-- giveThisAnIdTo style it in ../public/assets/css/style.css
                            or a class... example #xy
                       --}}
                         <li id='#xy'>{{this.name}}</li>
                         <p>{{this.description}}</p> 
                      {{/each}}
                    <ul>
                    {{/if}}
      </div>
    </div>
  </div>
</div>

        <div class="row">
          <div class="col s12 m6 offset-m3">
            <div class="card blue-grey darken-1">
              <div class="card-content white-text">
                <span class="card-title">Description</span>
                <p>{{description}}</p>
              </div>
            </div>
          </div>
        </div>

        <div class="row">
        <div class="col s12 m6 offset-m3">
            <div class="card blue-grey darken-1">
              <div class="card-content white-text">
                <span class="card-title">Text Examples</span>
                <p>{{examples.text}}</p>
              </div>
            </div>
          </div>
      </div>

        <div class="row">
          <div class="col s12 m6 offset-m3">
            <div class="card blue-grey darken-1">
              <div class="card-content white-text">
                <span class="card-title">Comments</span>
                <p>Mario: Comment 1</p>
                <br>
                <br>
                <p style="font-size: 10px">Juan: this is a Reply to Comment 1</p>
                <br />
                <br />
                <a href="#">Reply</a>
              </div>
            </div>
          </div>
        </div>

</main>
